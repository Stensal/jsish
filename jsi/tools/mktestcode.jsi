#!/usr/bin/env jsish
// Create the samples index file for the wiki.

var rc = '<h2>Javascript Tests</h2>\nFollowing are links to all of the javascript test-files.<p>';
var lst = File.glob('../tests/*.js');

for (var i in lst.sort()) {
    var n = lst[i];
    rc += '[../tests/'+n+'|'+n+']\n';
}
puts(rc);

